<h1>Contacts#index</h1>
<p>Find me in app/views/contacts/index.html.erb</p>


<div class="container">
	<div class="row">
		
	<% @contacts.each do |contact| %>
		<div class="row">
			<a href="/users/<%= @user.id %>/contacts/<%= contact.id %>"><%= contact.first_name + " " + contact.last_name %></a>
			<a href="/users/<%= @user.id %>/contacts/<%= contact.id %>"><%= contact.phone_num %></a>
		</div>
	<% end %>

	</div>
</div>